{
  "name": "json-rules-poc-cli",
  "version": "1.3.0",
  "description": "CLI utility to test Rules engine",
  "main": "dist/json-rules-poc-cli.js",
  "scripts": {
    "build": "npm run clean; npm ci",
    "clean": "rm -rf reports/ .webpack/ dist/ .nyc_output/",
    "compile": "npx webpack --config webpack.config.js",
    "exec": "node --require ts-node/register src/index.ts -f ./data/facts.json -r ./data/rules.json",
    "deploy": "npm run compile;"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^2.19.0",
    "json-rules-engine": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^8.10.37",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
